# Claude API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Database Configuration
DATABASE_PATH=./data/researchdb.sqlite

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=./logs/researchdb.log

# Agent Configuration
EXPLORER_MODEL=claude-sonnet-4
RESEARCHER_MODEL=claude-sonnet-4
